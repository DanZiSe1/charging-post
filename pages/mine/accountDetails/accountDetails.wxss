.detailsPage{
  padding: 0 40rpx 0 42rpx;
  box-sizing: border-box;
  color: #333;
}
.detailsList{
  padding: 18rpx 0;
  box-sizing: border-box;
  border-bottom: 1px solid #ddd;
  font-size: 28rpx;
}
.date{color: #999;}
.way{color: #003049;}
.consumption{color: #D62828;}
.amount{color: #F77F00;}
.listTop{
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 12rpx;
}
.noMore{
  text-align: center;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
}