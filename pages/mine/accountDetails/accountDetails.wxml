<!--pages/mine/accountDetails/accountDetails.wxml-->
<view class="detailsPage">
  <view wx:if="{{rechargeList.length != 0}}">
    <view class="detailsList" wx:for="{{detailsList}}" wx:key="detailsList">
      <view class="listTop">
        <view class="date">2020-02</view>
        <view class="amount">+300</view>
      </view>
      <view class="way">充值</view>
    </view>
  </view>
  <block wx:else>
    <noInfo id="noInfo"></noInfo>
  </block>
</view>
