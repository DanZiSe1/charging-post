.text-yellow{
  color:#FCBF49;
}
.rechargePage{
  padding: 0 26rpx 0 56rpx;
  box-sizing: border-box;
  color: #999;
  font-size: 28rpx;
}

.status{
  display: flex;
  flex-direction: column;
  align-items: center;
  font-weight: bold;
  padding: 34rpx 0 38rpx 0;
  box-sizing: border-box;
  color:#003049;
  font-size: 32rpx;
}
.charging_result{
  width: 88rpx;
  height: 88rpx;
  margin-bottom:20rpx;
}
.charging_result image{
  width:100%;
  height:100%;
}
.detilsInfo{
  margin-bottom: 44rpx;
}
.expense{
  display: flex;
  justify-content: space-between;
  border-bottom: 2rpx solid #D8D8D8;
  padding-bottom: 20rpx;
}
.expenseInfo{
  font-size: 32rpx;
  color:#003049;
}
.expenseDetails{
  display: flex;
  align-items: center;
  color:#FCBF49;
}

.project{
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top:20rpx;
  padding-right:20rpx;
}