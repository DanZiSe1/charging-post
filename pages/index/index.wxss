/**index.wxss**/
.flex-center{
  display: flex;
  align-items: center;
}
.flex-center-between{
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.container,.chargingPage{
  width:100%;
}
.chargingItem{
  width:100%;
  padding:10px 30px;
  border-bottom:1px solid #ddd;
  box-sizing: border-box;
}
.item_top_left h4{
  display: block;
  font-size: 16px;
}
.item_top_left p{
  font-size: 14px;
}
